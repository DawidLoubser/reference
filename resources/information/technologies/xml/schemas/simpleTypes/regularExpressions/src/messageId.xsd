<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs='http://www.w3.org/2001/XMLSchema'>

  <xs:simpleType name="GautengVehicleRegistration">
    <xs:restriction base="xs:string">
      <xs:pattern value="[A-Z]{3} [0-9]{3} GP"/>
    </xs:restriction>
  </xs:simpleType>
    
  <xs:element name="registration" type="GautengVehicleRegistration"/>
    
</xs:schema>
