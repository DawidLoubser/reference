<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" 
    targetNamespace="http://www.solms.co.za/customerList"
    xmlns="http://www.solms.co.za/customerList"
    xmlns:ent="http://www.solms.co.za/entities"
    elementFormDefault="qualified" attributeFormDefault="unqualified"> 
    
    <xs:import namespace="http://www.solms.co.za/entities" schemaLocation="./entities.xsd"/>
    
    <xs:complexType name="CustomerList">
        <xs:sequence>
            <xs:element name="customer" type="ent:LegalEntity" maxOccurs="unbounded"/> 
        </xs:sequence>
    </xs:complexType>    
 
</xs:schema>
