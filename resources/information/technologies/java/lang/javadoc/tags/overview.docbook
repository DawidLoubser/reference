<?xml version="1.0" encoding="UTF-8"?><?oxygen RNGSchema="http://www.oasis-open.org/docbook/xml/5.0/rng/docbookxi.rng" type="xml"?>
<ns6840:section xmlns:ns6840="http://docbook.org/ns/docbook" xml:id="resources.information.technologies.java.lang.javadoc.tags.overview">
  <ns6841:title xmlns:ns6841="http://docbook.org/ns/docbook">Overview</ns6841:title>
  <ns6842:para xmlns:ns6842="http://docbook.org/ns/docbook">
    Lets look first at the purpose of some of the most common JavaDoc tags.
  </ns6842:para>  
  <ns6843:table xmlns:ns6843="http://docbook.org/ns/docbook">
    <ns6844:title xmlns:ns6844="http://docbook.org/ns/docbook">Overview of the JavaDoc tags</ns6844:title>
    <ns6845:tgroup xmlns:ns6845="http://docbook.org/ns/docbook" cols="2">
      <ns6846:thead xmlns:ns6846="http://docbook.org/ns/docbook">
        <ns6847:row xmlns:ns6847="http://docbook.org/ns/docbook">
          <ns6848:entry xmlns:ns6848="http://docbook.org/ns/docbook">
            <ns6849:para xmlns:ns6849="http://docbook.org/ns/docbook">JavaDoc Tag</ns6849:para>
          </ns6848:entry>
          <ns6850:entry xmlns:ns6850="http://docbook.org/ns/docbook">
            <ns6851:para xmlns:ns6851="http://docbook.org/ns/docbook">Purpose</ns6851:para>
          </ns6850:entry>
        </ns6847:row>
      </ns6846:thead>
      <ns6852:tbody xmlns:ns6852="http://docbook.org/ns/docbook">
        <ns6853:row xmlns:ns6853="http://docbook.org/ns/docbook">
          <ns6854:entry xmlns:ns6854="http://docbook.org/ns/docbook"><ns6855:para xmlns:ns6855="http://docbook.org/ns/docbook">
            <ns6856:literal xmlns:ns6856="http://docbook.org/ns/docbook">@author</ns6856:literal>
          </ns6855:para></ns6854:entry>
          <ns6857:entry xmlns:ns6857="http://docbook.org/ns/docbook"><ns6858:para xmlns:ns6858="http://docbook.org/ns/docbook">
            Specify one or more authors for a component.
          </ns6858:para></ns6857:entry>
        </ns6853:row>
        <ns6859:row xmlns:ns6859="http://docbook.org/ns/docbook">
          <ns6860:entry xmlns:ns6860="http://docbook.org/ns/docbook"><ns6861:para xmlns:ns6861="http://docbook.org/ns/docbook">
            <ns6862:literal xmlns:ns6862="http://docbook.org/ns/docbook">@deprecated</ns6862:literal>
          </ns6861:para></ns6860:entry>
          <ns6863:entry xmlns:ns6863="http://docbook.org/ns/docbook"><ns6864:para xmlns:ns6864="http://docbook.org/ns/docbook">
            Specify that a certain component or service should no longer be used.
          </ns6864:para></ns6863:entry>
        </ns6859:row>
        <ns6865:row xmlns:ns6865="http://docbook.org/ns/docbook">
          <ns6866:entry xmlns:ns6866="http://docbook.org/ns/docbook"><ns6867:para xmlns:ns6867="http://docbook.org/ns/docbook">
            <ns6868:literal xmlns:ns6868="http://docbook.org/ns/docbook">@throws</ns6868:literal> or <ns6869:literal xmlns:ns6869="http://docbook.org/ns/docbook">@exception</ns6869:literal>
          </ns6867:para></ns6866:entry>
          <ns6870:entry xmlns:ns6870="http://docbook.org/ns/docbook"><ns6871:para xmlns:ns6871="http://docbook.org/ns/docbook">
            Specify that a service may throw a particular exception.
          </ns6871:para></ns6870:entry>
        </ns6865:row>
        <ns6872:row xmlns:ns6872="http://docbook.org/ns/docbook">
          <ns6873:entry xmlns:ns6873="http://docbook.org/ns/docbook"><ns6874:para xmlns:ns6874="http://docbook.org/ns/docbook">
            <ns6875:literal xmlns:ns6875="http://docbook.org/ns/docbook">{@link}</ns6875:literal>
          </ns6874:para></ns6873:entry>
          <ns6876:entry xmlns:ns6876="http://docbook.org/ns/docbook"><ns6877:para xmlns:ns6877="http://docbook.org/ns/docbook">
            Specifies an in-line link to another document or to a section in the same
            or in another document.
          </ns6877:para></ns6876:entry>
        </ns6872:row>
        <ns6878:row xmlns:ns6878="http://docbook.org/ns/docbook">
          <ns6879:entry xmlns:ns6879="http://docbook.org/ns/docbook"><ns6880:para xmlns:ns6880="http://docbook.org/ns/docbook">
            <ns6881:literal xmlns:ns6881="http://docbook.org/ns/docbook">@param</ns6881:literal>
          </ns6880:para></ns6879:entry>
          <ns6882:entry xmlns:ns6882="http://docbook.org/ns/docbook"><ns6883:para xmlns:ns6883="http://docbook.org/ns/docbook">
            Designates the description of a method parameter.            
          </ns6883:para></ns6882:entry>
        </ns6878:row>
        <ns6884:row xmlns:ns6884="http://docbook.org/ns/docbook">
          <ns6885:entry xmlns:ns6885="http://docbook.org/ns/docbook"><ns6886:para xmlns:ns6886="http://docbook.org/ns/docbook">
            <ns6887:literal xmlns:ns6887="http://docbook.org/ns/docbook">@return</ns6887:literal>
          </ns6886:para></ns6885:entry>
          <ns6888:entry xmlns:ns6888="http://docbook.org/ns/docbook"><ns6889:para xmlns:ns6889="http://docbook.org/ns/docbook">
            Designates the description of a return value.
          </ns6889:para></ns6888:entry>
        </ns6884:row>
        <ns6890:row xmlns:ns6890="http://docbook.org/ns/docbook">
          <ns6891:entry xmlns:ns6891="http://docbook.org/ns/docbook"><ns6892:para xmlns:ns6892="http://docbook.org/ns/docbook">
             <ns6893:literal xmlns:ns6893="http://docbook.org/ns/docbook">@see</ns6893:literal>
          </ns6892:para></ns6891:entry>
          <ns6894:entry xmlns:ns6894="http://docbook.org/ns/docbook"><ns6895:para xmlns:ns6895="http://docbook.org/ns/docbook">
            Specifies a link which will be placed in the <ns6896:emphasis xmlns:ns6896="http://docbook.org/ns/docbook">See also</ns6896:emphasis> section.
          </ns6895:para></ns6894:entry>
        </ns6890:row>
        <ns6897:row xmlns:ns6897="http://docbook.org/ns/docbook">
          <ns6898:entry xmlns:ns6898="http://docbook.org/ns/docbook"><ns6899:para xmlns:ns6899="http://docbook.org/ns/docbook">
             <ns6900:literal xmlns:ns6900="http://docbook.org/ns/docbook">@serial</ns6900:literal>
          </ns6899:para></ns6898:entry>
          <ns6901:entry xmlns:ns6901="http://docbook.org/ns/docbook"><ns6902:para xmlns:ns6902="http://docbook.org/ns/docbook">
            This tag is used together with the <ns6903:literal xmlns:ns6903="http://docbook.org/ns/docbook">@serialField</ns6903:literal> tag
            to specify serialization information.
          </ns6902:para></ns6901:entry>
        </ns6897:row>
        <ns6904:row xmlns:ns6904="http://docbook.org/ns/docbook">
          <ns6905:entry xmlns:ns6905="http://docbook.org/ns/docbook"><ns6906:para xmlns:ns6906="http://docbook.org/ns/docbook">
             <ns6907:literal xmlns:ns6907="http://docbook.org/ns/docbook">@since</ns6907:literal>
          </ns6906:para></ns6905:entry>
          <ns6908:entry xmlns:ns6908="http://docbook.org/ns/docbook"><ns6909:para xmlns:ns6909="http://docbook.org/ns/docbook">
            Used to annotate that a particular feature is available only since a particular version.
          </ns6909:para></ns6908:entry>
        </ns6904:row>
        <ns6910:row xmlns:ns6910="http://docbook.org/ns/docbook">
          <ns6911:entry xmlns:ns6911="http://docbook.org/ns/docbook"><ns6912:para xmlns:ns6912="http://docbook.org/ns/docbook">
             <ns6913:literal xmlns:ns6913="http://docbook.org/ns/docbook">@version</ns6913:literal>
          </ns6912:para></ns6911:entry>
          <ns6914:entry xmlns:ns6914="http://docbook.org/ns/docbook"><ns6915:para xmlns:ns6915="http://docbook.org/ns/docbook">
            Specifies the version of the component.
          </ns6915:para></ns6914:entry>
        </ns6910:row>
      </ns6852:tbody>      
    </ns6845:tgroup>
  </ns6843:table>
  
</ns6840:section>