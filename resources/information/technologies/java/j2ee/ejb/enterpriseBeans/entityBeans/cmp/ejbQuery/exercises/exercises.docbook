<?xml version="1.0" encoding="UTF-8"?><?oxygen RNGSchema="http://www.oasis-open.org/docbook/xml/5.0/rng/docbookxi.rng" type="xml"?>
<ns1079:section xmlns:ns1079="http://docbook.org/ns/docbook" xml:id="resources.information.technologies.java.j2ee.ejb.enterpriseBeans.entityBeans.cmp.ejbQuery.exercises.exercises">

    <ns1080:title xmlns:ns1080="http://docbook.org/ns/docbook">Exercises</ns1080:title>
    <ns1081:orderedlist xmlns:ns1081="http://docbook.org/ns/docbook">
        <ns1082:listitem xmlns:ns1082="http://docbook.org/ns/docbook">
          <ns1083:para xmlns:ns1083="http://docbook.org/ns/docbook">Provide the following finder methods for your parts catalog: </ns1083:para>
          <ns1084:orderedlist xmlns:ns1084="http://docbook.org/ns/docbook">
            <ns1085:listitem xmlns:ns1085="http://docbook.org/ns/docbook">
              <ns1086:para xmlns:ns1086="http://docbook.org/ns/docbook">Find all parts for a given manufacturer. </ns1086:para>
            </ns1085:listitem>
            <ns1087:listitem xmlns:ns1087="http://docbook.org/ns/docbook">
              <ns1088:para xmlns:ns1088="http://docbook.org/ns/docbook">Find those parts which are cheaper/more expensive than some supplied threshold
                price. </ns1088:para>
            </ns1087:listitem>
            <ns1089:listitem xmlns:ns1089="http://docbook.org/ns/docbook">
              <ns1090:para xmlns:ns1090="http://docbook.org/ns/docbook">Find all parts which have further sub-parts. </ns1090:para>
            </ns1089:listitem>
            <ns1091:listitem xmlns:ns1091="http://docbook.org/ns/docbook">
              <ns1092:para xmlns:ns1092="http://docbook.org/ns/docbook">Find all manufacturers which manufacturer at least 10 of the parts contained in
                the parts catalog. </ns1092:para>
            </ns1091:listitem>
            <ns1093:listitem xmlns:ns1093="http://docbook.org/ns/docbook">
              <ns1094:para xmlns:ns1094="http://docbook.org/ns/docbook">Find all manufacturers which manufacturer at least 10 composite parts, i.e.
                parts contained of further parts.</ns1094:para>
            </ns1093:listitem>
          </ns1084:orderedlist>
          <ns1095:para xmlns:ns1095="http://docbook.org/ns/docbook">Redeploy your enterprise beans and write a client which tests these.</ns1095:para>
        </ns1082:listitem>
      </ns1081:orderedlist>
    </ns1079:section>