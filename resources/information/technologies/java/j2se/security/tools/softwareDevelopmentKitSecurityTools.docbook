<ns3820:section xmlns:ns3820="http://docbook.org/ns/docbook" xml:id="resources.information.technologies.java.j2se.security.tools.softwareDevelopmentKitSecurityTools">
    
    <ns3821:title xmlns:ns3821="http://docbook.org/ns/docbook">Software Development Kit (SDK)
        Security Tools</ns3821:title>
    <ns3822:para xmlns:ns3822="http://docbook.org/ns/docbook"> The Java 2 SDK comes packed with 3 security tools: <ns3823:itemizedlist xmlns:ns3823="http://docbook.org/ns/docbook">
            <ns3824:listitem xmlns:ns3824="http://docbook.org/ns/docbook">
                <ns3825:para xmlns:ns3825="http://docbook.org/ns/docbook">
                    <ns3826:emphasis xmlns:ns3826="http://docbook.org/ns/docbook" role="verb">keytool</ns3826:emphasis>
                    <ns3827:itemizedlist xmlns:ns3827="http://docbook.org/ns/docbook">
                        <ns3828:listitem xmlns:ns3828="http://docbook.org/ns/docbook">
                            <ns3829:para xmlns:ns3829="http://docbook.org/ns/docbook">A tool for generating and managing public and private keys and certificates.</ns3829:para>
                        </ns3828:listitem>
                    </ns3827:itemizedlist>
                </ns3825:para>
            </ns3824:listitem>
            <ns3830:listitem xmlns:ns3830="http://docbook.org/ns/docbook">
                <ns3831:para xmlns:ns3831="http://docbook.org/ns/docbook">
                    <ns3832:emphasis xmlns:ns3832="http://docbook.org/ns/docbook" role="verb">jarsigner</ns3832:emphasis>
                    <ns3833:itemizedlist xmlns:ns3833="http://docbook.org/ns/docbook">
                        <ns3834:listitem xmlns:ns3834="http://docbook.org/ns/docbook">
                            <ns3835:para xmlns:ns3835="http://docbook.org/ns/docbook">An application for generating and verifying jar signatures.</ns3835:para>
                        </ns3834:listitem>
                    </ns3833:itemizedlist>
                </ns3831:para>
            </ns3830:listitem>
            <ns3836:listitem xmlns:ns3836="http://docbook.org/ns/docbook">
                <ns3837:para xmlns:ns3837="http://docbook.org/ns/docbook">
                    <ns3838:emphasis xmlns:ns3838="http://docbook.org/ns/docbook" role="verb">policytool</ns3838:emphasis>
                    <ns3839:itemizedlist xmlns:ns3839="http://docbook.org/ns/docbook">
                        <ns3840:listitem xmlns:ns3840="http://docbook.org/ns/docbook">
                            <ns3841:para xmlns:ns3841="http://docbook.org/ns/docbook">A GUI-based tool for managing security policies.</ns3841:para>
                        </ns3840:listitem>
                    </ns3839:itemizedlist>
                </ns3837:para>
            </ns3836:listitem>
        </ns3823:itemizedlist>
    </ns3822:para>
    
