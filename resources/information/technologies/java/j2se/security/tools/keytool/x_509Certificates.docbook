<?xml version="1.0" encoding="UTF-8"?><?oxygen RNGSchema="http://www.oasis-open.org/docbook/xml/5.0/rng/docbookxi.rng" type="xml"?>
<ns3779:section xmlns:ns3779="http://docbook.org/ns/docbook" xml:id="resources.information.technologies.java.j2se.security.tools.keytool.x_509Certificates">
    <ns3780:title xmlns:ns3780="http://docbook.org/ns/docbook">X.509 certificates</ns3780:title>
    <ns3781:para xmlns:ns3781="http://docbook.org/ns/docbook">
        <ns3782:emphasis xmlns:ns3782="http://docbook.org/ns/docbook">X.509</ns3782:emphasis>} is a standard for authentication information. Every X.509 contains:<ns3783:itemizedlist xmlns:ns3783="http://docbook.org/ns/docbook">
            <ns3784:listitem xmlns:ns3784="http://docbook.org/ns/docbook">
                <ns3785:formalpara xmlns:ns3785="http://docbook.org/ns/docbook">
                    <ns3786:title xmlns:ns3786="http://docbook.org/ns/docbook">Version</ns3786:title>
                    <ns3787:para xmlns:ns3787="http://docbook.org/ns/docbook">the version of X.509 which the certificate complies to,</ns3787:para>
                </ns3785:formalpara>
            </ns3784:listitem>
            <ns3788:listitem xmlns:ns3788="http://docbook.org/ns/docbook">
                <ns3789:formalpara xmlns:ns3789="http://docbook.org/ns/docbook">
                    <ns3790:title xmlns:ns3790="http://docbook.org/ns/docbook">Serial Number</ns3790:title>
                    <ns3791:para xmlns:ns3791="http://docbook.org/ns/docbook">the serial number of the certificate must be unique for the issuer,</ns3791:para>
                </ns3789:formalpara>
            </ns3788:listitem>
            <ns3792:listitem xmlns:ns3792="http://docbook.org/ns/docbook">
                <ns3793:formalpara xmlns:ns3793="http://docbook.org/ns/docbook">
                    <ns3794:title xmlns:ns3794="http://docbook.org/ns/docbook">Signature Algorithm Identifier</ns3794:title>
                    <ns3795:para xmlns:ns3795="http://docbook.org/ns/docbook"> specifies the algorithm used to sign the certificate,</ns3795:para>
                </ns3793:formalpara>
            </ns3792:listitem>
            <ns3796:listitem xmlns:ns3796="http://docbook.org/ns/docbook">
                <ns3797:formalpara xmlns:ns3797="http://docbook.org/ns/docbook">
                    <ns3798:title xmlns:ns3798="http://docbook.org/ns/docbook">Issuer Name</ns3798:title>
                    <ns3799:para xmlns:ns3799="http://docbook.org/ns/docbook">the  <ns3800:emphasis xmlns:ns3800="http://docbook.org/ns/docbook">X.500 distinguished name</ns3800:emphasis> of the private key which was used to
                        sign the certificate,</ns3799:para>
                </ns3797:formalpara>
            </ns3796:listitem>
            <ns3801:listitem xmlns:ns3801="http://docbook.org/ns/docbook">
                <ns3802:formalpara xmlns:ns3802="http://docbook.org/ns/docbook">
                    <ns3803:title xmlns:ns3803="http://docbook.org/ns/docbook">Validity Period</ns3803:title>
                    <ns3804:para xmlns:ns3804="http://docbook.org/ns/docbook"> for which the certificate will be valid which may range from a few
                        seconds to a number of years (depending on factors like the strength of the
                        private key and the security requirements of the signer),</ns3804:para>
                </ns3802:formalpara>
            </ns3801:listitem>
            <ns3805:listitem xmlns:ns3805="http://docbook.org/ns/docbook">
                <ns3806:formalpara xmlns:ns3806="http://docbook.org/ns/docbook">
                    <ns3807:title xmlns:ns3807="http://docbook.org/ns/docbook">Subject Name</ns3807:title>
                    <ns3808:para xmlns:ns3808="http://docbook.org/ns/docbook">the name of the entity which the certificate identifies and</ns3808:para>
                </ns3806:formalpara>
            </ns3805:listitem>
            <ns3809:listitem xmlns:ns3809="http://docbook.org/ns/docbook">
                <ns3810:formalpara xmlns:ns3810="http://docbook.org/ns/docbook">
                    <ns3811:title xmlns:ns3811="http://docbook.org/ns/docbook">Subject Public Key Information</ns3811:title>
                    <ns3812:para xmlns:ns3812="http://docbook.org/ns/docbook">containing <ns3813:itemizedlist xmlns:ns3813="http://docbook.org/ns/docbook">
                            <ns3814:listitem xmlns:ns3814="http://docbook.org/ns/docbook">
                                <ns3815:para xmlns:ns3815="http://docbook.org/ns/docbook">the public key itself,</ns3815:para>
                            </ns3814:listitem>
                            <ns3816:listitem xmlns:ns3816="http://docbook.org/ns/docbook">
                                <ns3817:para xmlns:ns3817="http://docbook.org/ns/docbook">the cryptographic algorithm used to generate the key and</ns3817:para>
                            </ns3816:listitem>
                            <ns3818:listitem xmlns:ns3818="http://docbook.org/ns/docbook">
                                <ns3819:para xmlns:ns3819="http://docbook.org/ns/docbook">the algorithm parameters</ns3819:para>
                            </ns3818:listitem>
                        </ns3813:itemizedlist>
                    </ns3812:para>
                </ns3810:formalpara>
            </ns3809:listitem>
        </ns3783:itemizedlist>
    </ns3781:para>
</ns3779:section>