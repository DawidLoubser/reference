<programlisting
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:noNamespaceSchemaLocation="http://www.solms.co.za/schemas/doc/solmsDoc/solmsDoc.xsd" 
    xmlns:xi="http://www.w3.org/2001/XInclude">
    <![CDATA[
#!/bin/bash
echo "Enter x:"; read x
echo "Enter y:"; read y

echo "x*y = `expr $x '*' $y`"

echo "x*y = $[x*y]"
]]>
    </programlisting>
