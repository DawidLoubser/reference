                <ns2166:listitem xmlns:ns2166="http://docbook.org/ns/docbook"><ns2167:para xmlns:ns2167="http://docbook.org/ns/docbook">
                    The RAID configuration.
                </ns2167:para></ns2166:listitem>
            </ns2161:itemizedlist>
        </ns2160:para>
    </ns2158:section>

    <ns2168:section xmlns:ns2168="http://docbook.org/ns/docbook">
