<!--DOCTYPE section SYSTEM "file:/opt/share/xml/Oxygen/docbook/dtd/docbookx.dtd"--><ns6400:section xmlns:ns6400="http://docbook.org/ns/docbook" xml:id="resources.information.platforms.linux.vi.deleting">

  <ns6401:title xmlns:ns6401="http://docbook.org/ns/docbook">Deleting or cutting</ns6401:title>
  <ns6402:para xmlns:ns6402="http://docbook.org/ns/docbook">
    Deleted text is inserted into the buffer and can be pasted via <ns6403:emphasis xmlns:ns6403="http://docbook.org/ns/docbook">Vim</ns6403:emphasis>'s
    put command at a later stage.
  </ns6402:para>
  <ns6404:table xmlns:ns6404="http://docbook.org/ns/docbook" xml:id="resources.information.platforms.linux.vi.deleteCommands">
    <ns6405:title xmlns:ns6405="http://docbook.org/ns/docbook">Delete commands</ns6405:title>
    <ns6406:tgroup xmlns:ns6406="http://docbook.org/ns/docbook" cols="2">
      <ns6407:thead xmlns:ns6407="http://docbook.org/ns/docbook">
        <ns6408:row xmlns:ns6408="http://docbook.org/ns/docbook"><ns6409:entry xmlns:ns6409="http://docbook.org/ns/docbook">Command</ns6409:entry><ns6410:entry xmlns:ns6410="http://docbook.org/ns/docbook">Description</ns6410:entry></ns6408:row>
      </ns6407:thead>
      <ns6411:tbody xmlns:ns6411="http://docbook.org/ns/docbook">
        <ns6412:row xmlns:ns6412="http://docbook.org/ns/docbook">
          <ns6413:entry xmlns:ns6413="http://docbook.org/ns/docbook">["{0-9}][n]x , [n]&lt;del&gt;</ns6413:entry>
          <ns6414:entry xmlns:ns6414="http://docbook.org/ns/docbook">Delete n characters from cursor position and insert into register {0-9}.</ns6414:entry>
        </ns6412:row>
        <ns6415:row xmlns:ns6415="http://docbook.org/ns/docbook">
          <ns6416:entry xmlns:ns6416="http://docbook.org/ns/docbook">["{0-9}][n]dd</ns6416:entry>
          <ns6417:entry xmlns:ns6417="http://docbook.org/ns/docbook">Delete n next lines and insert into register {0-9}.</ns6417:entry>
        </ns6415:row>
        <ns6418:row xmlns:ns6418="http://docbook.org/ns/docbook">
          <ns6419:entry xmlns:ns6419="http://docbook.org/ns/docbook">["{0-9}][n]D</ns6419:entry>
          <ns6420:entry xmlns:ns6420="http://docbook.org/ns/docbook">Delete till end of line (and n lines thereafter) and insert into register {0-9}.</ns6420:entry>
        </ns6418:row>
        <ns6421:row xmlns:ns6421="http://docbook.org/ns/docbook">
          <ns6422:entry xmlns:ns6422="http://docbook.org/ns/docbook">[n]J</ns6422:entry>
          <ns6423:entry xmlns:ns6423="http://docbook.org/ns/docbook">Join the next n (default 2) line (delete the EOL characters).</ns6423:entry>
        </ns6421:row>
        <ns6424:row xmlns:ns6424="http://docbook.org/ns/docbook">
          <ns6425:entry xmlns:ns6425="http://docbook.org/ns/docbook">[n]gJ</ns6425:entry>
          <ns6426:entry xmlns:ns6426="http://docbook.org/ns/docbook">Join the next n (default 2) line without inserting spaces.</ns6426:entry>
        </ns6424:row>
      </ns6411:tbody>
    </ns6406:tgroup>
    
  </ns6404:table>
  
</ns6400:section>