<!--DOCTYPE section SYSTEM "file:/opt/share/xml/Oxygen/docbook/dtd/docbookx.dtd"--><ns6636:section xmlns:ns6636="http://docbook.org/ns/docbook" xml:id="resources.information.platforms.linux.vi.navigating">

  <ns6637:title xmlns:ns6637="http://docbook.org/ns/docbook">Navigating through a document</ns6637:title>
  <ns6638:para xmlns:ns6638="http://docbook.org/ns/docbook">
    <ns6639:emphasis xmlns:ns6639="http://docbook.org/ns/docbook">Vi</ns6639:emphasis> provides efficient navigation mechanisms, even on minimalistic
    systems, without a mouse or a modern keyboard.
  </ns6638:para>
  
  <ns6640:table xmlns:ns6640="http://docbook.org/ns/docbook">
    <ns6641:title xmlns:ns6641="http://docbook.org/ns/docbook">Left-right navigation commands.</ns6641:title>
    <ns6642:tgroup xmlns:ns6642="http://docbook.org/ns/docbook" cols="2">
      <ns6643:thead xmlns:ns6643="http://docbook.org/ns/docbook">
        <ns6644:row xmlns:ns6644="http://docbook.org/ns/docbook"><ns6645:entry xmlns:ns6645="http://docbook.org/ns/docbook">Command</ns6645:entry><ns6646:entry xmlns:ns6646="http://docbook.org/ns/docbook">Description</ns6646:entry></ns6644:row>
      </ns6643:thead>
      <ns6647:tbody xmlns:ns6647="http://docbook.org/ns/docbook">
        <ns6648:row xmlns:ns6648="http://docbook.org/ns/docbook">
          <ns6649:entry xmlns:ns6649="http://docbook.org/ns/docbook">[n]&lt;Ctrl&gt;h , [n]&lt;left-arrow&gt;</ns6649:entry>
          <ns6650:entry xmlns:ns6650="http://docbook.org/ns/docbook">Move (n characters) to the left.</ns6650:entry>
        </ns6648:row>
        <ns6651:row xmlns:ns6651="http://docbook.org/ns/docbook">
          <ns6652:entry xmlns:ns6652="http://docbook.org/ns/docbook">[n]&lt;Ctrl&gt;l , [n]&lt;right-arrow&gt;</ns6652:entry>
          <ns6653:entry xmlns:ns6653="http://docbook.org/ns/docbook">Move (n characters) to the right.</ns6653:entry>
        </ns6651:row>
        <ns6654:row xmlns:ns6654="http://docbook.org/ns/docbook">
          <ns6655:entry xmlns:ns6655="http://docbook.org/ns/docbook">0</ns6655:entry>
          <ns6656:entry xmlns:ns6656="http://docbook.org/ns/docbook">Go to first character in line</ns6656:entry>
        </ns6654:row>
        <ns6657:row xmlns:ns6657="http://docbook.org/ns/docbook">
          <ns6658:entry xmlns:ns6658="http://docbook.org/ns/docbook">g0 , g$</ns6658:entry>
          <ns6659:entry xmlns:ns6659="http://docbook.org/ns/docbook">Go to first/last character on screen for that line. </ns6659:entry>
        </ns6657:row>
        <ns6660:row xmlns:ns6660="http://docbook.org/ns/docbook">
          <ns6661:entry xmlns:ns6661="http://docbook.org/ns/docbook">[n]|</ns6661:entry>
          <ns6662:entry xmlns:ns6662="http://docbook.org/ns/docbook">Go to column number n, 1 being the default.</ns6662:entry>
        </ns6660:row>
        <ns6663:row xmlns:ns6663="http://docbook.org/ns/docbook">
          <ns6664:entry xmlns:ns6664="http://docbook.org/ns/docbook">[n]f&lt;char&gt;</ns6664:entry>
          <ns6665:entry xmlns:ns6665="http://docbook.org/ns/docbook">Go to (first/n'th occurance) of that character to the right.</ns6665:entry>
        </ns6663:row>
        <ns6666:row xmlns:ns6666="http://docbook.org/ns/docbook">
          <ns6667:entry xmlns:ns6667="http://docbook.org/ns/docbook">[n]F&lt;char&gt;</ns6667:entry>
          <ns6668:entry xmlns:ns6668="http://docbook.org/ns/docbook">Go to (first/n'th occurance) of that character to the left.</ns6668:entry>
        </ns6666:row>
        <ns6669:row xmlns:ns6669="http://docbook.org/ns/docbook">
          <ns6670:entry xmlns:ns6670="http://docbook.org/ns/docbook">:go[n]</ns6670:entry>
          <ns6671:entry xmlns:ns6671="http://docbook.org/ns/docbook">Go to n'th byte in buffer.</ns6671:entry>
        </ns6669:row>
      </ns6647:tbody>
    </ns6642:tgroup>
  </ns6640:table>  
        
  <ns6672:table xmlns:ns6672="http://docbook.org/ns/docbook">
    <ns6673:title xmlns:ns6673="http://docbook.org/ns/docbook">Up/down navigation commands.</ns6673:title>
    <ns6674:tgroup xmlns:ns6674="http://docbook.org/ns/docbook" cols="2">
      <ns6675:thead xmlns:ns6675="http://docbook.org/ns/docbook">
        <ns6676:row xmlns:ns6676="http://docbook.org/ns/docbook"><ns6677:entry xmlns:ns6677="http://docbook.org/ns/docbook">Command</ns6677:entry><ns6678:entry xmlns:ns6678="http://docbook.org/ns/docbook">Description</ns6678:entry></ns6676:row>
      </ns6675:thead>
      <ns6679:tbody xmlns:ns6679="http://docbook.org/ns/docbook">
        <ns6680:row xmlns:ns6680="http://docbook.org/ns/docbook">
          <ns6681:entry xmlns:ns6681="http://docbook.org/ns/docbook">[n]&lt;Ctrl&gt;k , [n]&lt;up-arrow&gt;</ns6681:entry>
          <ns6682:entry xmlns:ns6682="http://docbook.org/ns/docbook">Move (n lines) up.</ns6682:entry>
        </ns6680:row>
        <ns6683:row xmlns:ns6683="http://docbook.org/ns/docbook">
          <ns6684:entry xmlns:ns6684="http://docbook.org/ns/docbook">[n]&lt;Ctrl&gt;j , [n]&lt;down-arrow&gt;</ns6684:entry>
          <ns6685:entry xmlns:ns6685="http://docbook.org/ns/docbook">Move (n lines) down.</ns6685:entry>
        </ns6683:row>
        <ns6686:row xmlns:ns6686="http://docbook.org/ns/docbook">
          <ns6687:entry xmlns:ns6687="http://docbook.org/ns/docbook">[n]$ , [n]&lt;End&gt;</ns6687:entry>
          <ns6688:entry xmlns:ns6688="http://docbook.org/ns/docbook">Move to the end-of-line (n lines down).</ns6688:entry>
        </ns6686:row>
        <ns6689:row xmlns:ns6689="http://docbook.org/ns/docbook">
          <ns6690:entry xmlns:ns6690="http://docbook.org/ns/docbook">[n]+ , [n]-</ns6690:entry>
          <ns6691:entry xmlns:ns6691="http://docbook.org/ns/docbook">Go up/down (n lines), on first non-blank character.</ns6691:entry>
        </ns6689:row>
        <ns6692:row xmlns:ns6692="http://docbook.org/ns/docbook">
          <ns6693:entry xmlns:ns6693="http://docbook.org/ns/docbook">[n]_</ns6693:entry>
          <ns6694:entry xmlns:ns6694="http://docbook.org/ns/docbook">Go down (n-1 lines), on first non-blank character.</ns6694:entry>
        </ns6692:row>
        <ns6695:row xmlns:ns6695="http://docbook.org/ns/docbook">
          <ns6696:entry xmlns:ns6696="http://docbook.org/ns/docbook">[n]G , [n]gg</ns6696:entry>
          <ns6697:entry xmlns:ns6697="http://docbook.org/ns/docbook">Go down (n-1 lines), on first non-blank character.</ns6697:entry>
        </ns6695:row>
        <ns6698:row xmlns:ns6698="http://docbook.org/ns/docbook">
          <ns6699:entry xmlns:ns6699="http://docbook.org/ns/docbook">n%</ns6699:entry>
          <ns6700:entry xmlns:ns6700="http://docbook.org/ns/docbook">go to n% down in file.</ns6700:entry>
        </ns6698:row>
      </ns6679:tbody>
    </ns6674:tgroup>
  </ns6672:table>
  
  <ns6701:table xmlns:ns6701="http://docbook.org/ns/docbook">
    <ns6702:title xmlns:ns6702="http://docbook.org/ns/docbook">Text-based navigation.</ns6702:title>
    <ns6703:tgroup xmlns:ns6703="http://docbook.org/ns/docbook" cols="2">
      <ns6704:thead xmlns:ns6704="http://docbook.org/ns/docbook">
        <ns6705:row xmlns:ns6705="http://docbook.org/ns/docbook"><ns6706:entry xmlns:ns6706="http://docbook.org/ns/docbook">Command</ns6706:entry><ns6707:entry xmlns:ns6707="http://docbook.org/ns/docbook">Description</ns6707:entry></ns6705:row>
      </ns6704:thead>
      <ns6708:tbody xmlns:ns6708="http://docbook.org/ns/docbook">
        <ns6709:row xmlns:ns6709="http://docbook.org/ns/docbook">
          <ns6710:entry xmlns:ns6710="http://docbook.org/ns/docbook">[n]w , [n]e</ns6710:entry>
          <ns6711:entry xmlns:ns6711="http://docbook.org/ns/docbook">Go to the start/end of the n'th word, 1 being the default. </ns6711:entry>
        </ns6709:row>
        <ns6712:row xmlns:ns6712="http://docbook.org/ns/docbook">
          <ns6713:entry xmlns:ns6713="http://docbook.org/ns/docbook">[n]W , [n]E</ns6713:entry>
          <ns6714:entry xmlns:ns6714="http://docbook.org/ns/docbook">Go to the start/end of the n'th blank-separated word, 1 being the default. </ns6714:entry>
        </ns6712:row>
        <ns6715:row xmlns:ns6715="http://docbook.org/ns/docbook">
          <ns6716:entry xmlns:ns6716="http://docbook.org/ns/docbook">b , ge</ns6716:entry>
          <ns6717:entry xmlns:ns6717="http://docbook.org/ns/docbook">Go backwards to the start/end of the n'th word.</ns6717:entry>
        </ns6715:row>
        <ns6718:row xmlns:ns6718="http://docbook.org/ns/docbook">
          <ns6719:entry xmlns:ns6719="http://docbook.org/ns/docbook">B , gE</ns6719:entry>
          <ns6720:entry xmlns:ns6720="http://docbook.org/ns/docbook">Go backwards to the start/end of the n'th blank-separated word.</ns6720:entry>
        </ns6718:row>
        <ns6721:row xmlns:ns6721="http://docbook.org/ns/docbook">
          <ns6722:entry xmlns:ns6722="http://docbook.org/ns/docbook">[n]) , [n](</ns6722:entry>
          <ns6723:entry xmlns:ns6723="http://docbook.org/ns/docbook">Go to the start of the n'th sentence forward/backward.</ns6723:entry>
        </ns6721:row>
        <ns6724:row xmlns:ns6724="http://docbook.org/ns/docbook">
          <ns6725:entry xmlns:ns6725="http://docbook.org/ns/docbook">[n]} , [n]{</ns6725:entry>
          <ns6726:entry xmlns:ns6726="http://docbook.org/ns/docbook">Go to the start of the n'th paragraph forward/backward.</ns6726:entry>
        </ns6724:row>
      </ns6708:tbody>
    </ns6703:tgroup>
  </ns6701:table>
  
  <ns6727:table xmlns:ns6727="http://docbook.org/ns/docbook">
    <ns6728:title xmlns:ns6728="http://docbook.org/ns/docbook">Scrolling</ns6728:title>
    <ns6729:tgroup xmlns:ns6729="http://docbook.org/ns/docbook" cols="2">
      <ns6730:thead xmlns:ns6730="http://docbook.org/ns/docbook">
        <ns6731:row xmlns:ns6731="http://docbook.org/ns/docbook"><ns6732:entry xmlns:ns6732="http://docbook.org/ns/docbook">Command</ns6732:entry><ns6733:entry xmlns:ns6733="http://docbook.org/ns/docbook">Description</ns6733:entry></ns6731:row>
      </ns6730:thead>
      <ns6734:tbody xmlns:ns6734="http://docbook.org/ns/docbook">
        <ns6735:row xmlns:ns6735="http://docbook.org/ns/docbook">
          <ns6736:entry xmlns:ns6736="http://docbook.org/ns/docbook">[n]&lt;Ctrl&gt;e , [n]&lt;Ctrl&gt;y</ns6736:entry>
          <ns6737:entry xmlns:ns6737="http://docbook.org/ns/docbook">Scroll n lines (default 1) downward/upward.</ns6737:entry>
        </ns6735:row>
        <ns6738:row xmlns:ns6738="http://docbook.org/ns/docbook">
          <ns6739:entry xmlns:ns6739="http://docbook.org/ns/docbook">[n]&lt;Ctrl&gt;d , [n]&lt;Ctrl&gt;u</ns6739:entry>
          <ns6740:entry xmlns:ns6740="http://docbook.org/ns/docbook">Scroll n lines (default 1/2 page) downward/upward.</ns6740:entry>
        </ns6738:row>
        <ns6741:row xmlns:ns6741="http://docbook.org/ns/docbook">
          <ns6742:entry xmlns:ns6742="http://docbook.org/ns/docbook">[n]&lt;Ctrl&gt;f , [n]&lt;Ctrl&gt;b</ns6742:entry>
          <ns6743:entry xmlns:ns6743="http://docbook.org/ns/docbook">
            Scroll n pages (default 1) forward/backward. 
            <ns6744:note xmlns:ns6744="http://docbook.org/ns/docbook"><ns6745:para xmlns:ns6745="http://docbook.org/ns/docbook">
              You can also use 
              <ns6746:literal xmlns:ns6746="http://docbook.org/ns/docbook">&lt;PgUp&gt;</ns6746:literal>/<ns6747:literal xmlns:ns6747="http://docbook.org/ns/docbook">&lt;PgDwn&gt;</ns6747:literal>
              to scroll upward/downward.
            </ns6745:para></ns6744:note>
          </ns6743:entry>
        </ns6741:row>
        <ns6748:row xmlns:ns6748="http://docbook.org/ns/docbook">
          <ns6749:entry xmlns:ns6749="http://docbook.org/ns/docbook">[n]zl , [n]zh</ns6749:entry>
          <ns6750:entry xmlns:ns6750="http://docbook.org/ns/docbook">Scroll screen n (default 1) characters to left/right.</ns6750:entry>
        </ns6748:row>
        <ns6751:row xmlns:ns6751="http://docbook.org/ns/docbook">
          <ns6752:entry xmlns:ns6752="http://docbook.org/ns/docbook">[n]zL , [n]zH</ns6752:entry>
          <ns6753:entry xmlns:ns6753="http://docbook.org/ns/docbook">Scroll screen n (default 1) times 1/2 a screen to left/right.</ns6753:entry>
        </ns6751:row>
      </ns6734:tbody>
    </ns6729:tgroup>
    
  </ns6727:table>
  
  
</ns6636:section>