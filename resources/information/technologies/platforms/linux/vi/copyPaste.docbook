<!--DOCTYPE section SYSTEM "file:/opt/share/xml/Oxygen/docbook/dtd/docbookx.dtd"--><ns6367:section xmlns:ns6367="http://docbook.org/ns/docbook" xml:id="resources.information.platforms.linux.vi.copyPaste">

  <ns6368:title xmlns:ns6368="http://docbook.org/ns/docbook">Copying and pasting</ns6368:title>
  <ns6369:para xmlns:ns6369="http://docbook.org/ns/docbook">
    As you may have noticed, <ns6370:emphasis xmlns:ns6370="http://docbook.org/ns/docbook">Vim</ns6370:emphasis> often uses its own terminology
    for standard editing functions. This also holds true for copying and pasting.
  </ns6369:para>
  <ns6371:table xmlns:ns6371="http://docbook.org/ns/docbook">
    <ns6372:title xmlns:ns6372="http://docbook.org/ns/docbook">Copy and paste commands</ns6372:title>
    <ns6373:tgroup xmlns:ns6373="http://docbook.org/ns/docbook" cols="2">
      <ns6374:thead xmlns:ns6374="http://docbook.org/ns/docbook">
        <ns6375:row xmlns:ns6375="http://docbook.org/ns/docbook"><ns6376:entry xmlns:ns6376="http://docbook.org/ns/docbook">Command</ns6376:entry><ns6377:entry xmlns:ns6377="http://docbook.org/ns/docbook">Description</ns6377:entry></ns6375:row>
      </ns6374:thead>
      <ns6378:tbody xmlns:ns6378="http://docbook.org/ns/docbook">
        <ns6379:row xmlns:ns6379="http://docbook.org/ns/docbook">
            <ns6380:entry xmlns:ns6380="http://docbook.org/ns/docbook">["{0-9}][n]yy or ["{0-9}][n]Y</ns6380:entry>
            <ns6381:entry xmlns:ns6381="http://docbook.org/ns/docbook">Yank (copy) n lines (default 1) into register {0-9} (default 0).</ns6381:entry>
        </ns6379:row>
        <ns6382:row xmlns:ns6382="http://docbook.org/ns/docbook">
          <ns6383:entry xmlns:ns6383="http://docbook.org/ns/docbook">["{0-9}][n]y$</ns6383:entry>
          <ns6384:entry xmlns:ns6384="http://docbook.org/ns/docbook">Copy to end-of-line (and n lines more) into register {0-9} (default 0).</ns6384:entry>
        </ns6382:row>
        <ns6385:row xmlns:ns6385="http://docbook.org/ns/docbook">
          <ns6386:entry xmlns:ns6386="http://docbook.org/ns/docbook">["{0-9}][n]yw</ns6386:entry>
          <ns6387:entry xmlns:ns6387="http://docbook.org/ns/docbook">Copy next n words into register {0-9} (default 0).</ns6387:entry>
        </ns6385:row>
        <ns6388:row xmlns:ns6388="http://docbook.org/ns/docbook">
          <ns6389:entry xmlns:ns6389="http://docbook.org/ns/docbook">["{0-9}][n]p or ["{0-9}][n]p</ns6389:entry>
          <ns6390:entry xmlns:ns6390="http://docbook.org/ns/docbook">
            Put (paste) the content of register {0-9} (default 0) at the cursor position (n times).
          </ns6390:entry>
        </ns6388:row>
        <ns6391:row xmlns:ns6391="http://docbook.org/ns/docbook">
          <ns6392:entry xmlns:ns6392="http://docbook.org/ns/docbook">["{0-9}][n]p or ["{0-9}][n]p</ns6392:entry>
          <ns6393:entry xmlns:ns6393="http://docbook.org/ns/docbook">
            Put (paste) the content of register {0-9} (default 0) at the cursor position (n times),
            but adjust indenting to that of the current line.
          </ns6393:entry>
        </ns6391:row>
        <ns6394:row xmlns:ns6394="http://docbook.org/ns/docbook">
          <ns6395:entry xmlns:ns6395="http://docbook.org/ns/docbook">:reg</ns6395:entry>
          <ns6396:entry xmlns:ns6396="http://docbook.org/ns/docbook">Show the content of all registers.</ns6396:entry>
        </ns6394:row>
        <ns6397:row xmlns:ns6397="http://docbook.org/ns/docbook">
          <ns6398:entry xmlns:ns6398="http://docbook.org/ns/docbook">:reg {0-9}</ns6398:entry>
          <ns6399:entry xmlns:ns6399="http://docbook.org/ns/docbook">Show the content of register number {0-9}.</ns6399:entry>
        </ns6397:row>
      </ns6378:tbody>
    </ns6373:tgroup>
    
  </ns6371:table>
  
</ns6367:section>