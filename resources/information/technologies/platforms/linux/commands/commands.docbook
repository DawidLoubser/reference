<?xml version="1.0" encoding="UTF-8"?><?oxygen RNGSchema="http://www.oasis-open.org/docbook/xml/5.0b5/rng/docbookxi.rng" type="xml"?>
<ns360:section xmlns:ns360="http://docbook.org/ns/docbook" xml:id="resources.information.platforms.linux.commands.commands">

	<ns361:title xmlns:ns361="http://docbook.org/ns/docbook">Basic commands</ns361:title>
	<ns362:para xmlns:ns362="http://docbook.org/ns/docbook">
		
	</ns362:para>
	
	<ns363:section xmlns:ns363="http://docbook.org/ns/docbook">
		<ns364:title xmlns:ns364="http://docbook.org/ns/docbook">ls - Listing directory content</ns364:title>
		<ns365:para xmlns:ns365="http://docbook.org/ns/docbook">
		To list the contents to the current directory, the <ns366:emphasis xmlns:ns366="http://docbook.org/ns/docbook" role="strong">ls</ns366:emphasis> command is used.  
		</ns365:para>
		<ns367:screen xmlns:ns367="http://docbook.org/ns/docbook">
			<ns368:userinput xmlns:ns368="http://docbook.org/ns/docbook">
				<ns369:command xmlns:ns369="http://docbook.org/ns/docbook">ls</ns369:command> <ns370:replaceable xmlns:ns370="http://docbook.org/ns/docbook">flags</ns370:replaceable> <ns371:replaceable xmlns:ns371="http://docbook.org/ns/docbook">context</ns371:replaceable>
			</ns368:userinput>
		</ns367:screen>
		<ns372:table xmlns:ns372="http://docbook.org/ns/docbook" frame="all">
			<ns373:title xmlns:ns373="http://docbook.org/ns/docbook">ls Flags</ns373:title>
			<ns374:tgroup xmlns:ns374="http://docbook.org/ns/docbook" cols="2">
				<ns375:thead xmlns:ns375="http://docbook.org/ns/docbook">
					<ns376:row xmlns:ns376="http://docbook.org/ns/docbook"><ns377:entry xmlns:ns377="http://docbook.org/ns/docbook">Flag</ns377:entry><ns378:entry xmlns:ns378="http://docbook.org/ns/docbook">Description</ns378:entry></ns376:row>
				</ns375:thead>	
				<ns379:tbody xmlns:ns379="http://docbook.org/ns/docbook">
					<ns380:row xmlns:ns380="http://docbook.org/ns/docbook"><ns381:entry xmlns:ns381="http://docbook.org/ns/docbook">-a</ns381:entry><ns382:entry xmlns:ns382="http://docbook.org/ns/docbook">list all files, including hidden files</ns382:entry></ns380:row>
					<ns383:row xmlns:ns383="http://docbook.org/ns/docbook"><ns384:entry xmlns:ns384="http://docbook.org/ns/docbook">-A</ns384:entry><ns385:entry xmlns:ns385="http://docbook.org/ns/docbook">list almost all files including hidden files. The <ns386:emphasis xmlns:ns386="http://docbook.org/ns/docbook" role="strong">.</ns386:emphasis> and <ns387:emphasis xmlns:ns387="http://docbook.org/ns/docbook" role="strong">..</ns387:emphasis> are excluded.</ns385:entry></ns383:row>
					<ns388:row xmlns:ns388="http://docbook.org/ns/docbook"><ns389:entry xmlns:ns389="http://docbook.org/ns/docbook">-l</ns389:entry><ns390:entry xmlns:ns390="http://docbook.org/ns/docbook">includes file access permissions, size, ownership, and timestamp</ns390:entry></ns388:row>
					<ns391:row xmlns:ns391="http://docbook.org/ns/docbook"><ns392:entry xmlns:ns392="http://docbook.org/ns/docbook">-o</ns392:entry><ns393:entry xmlns:ns393="http://docbook.org/ns/docbook">same as -l, excluding group information</ns393:entry></ns391:row>
					<ns394:row xmlns:ns394="http://docbook.org/ns/docbook"><ns395:entry xmlns:ns395="http://docbook.org/ns/docbook">-g</ns395:entry><ns396:entry xmlns:ns396="http://docbook.org/ns/docbook">same as -l, excluding owner information</ns396:entry></ns394:row>
					<ns397:row xmlns:ns397="http://docbook.org/ns/docbook"><ns398:entry xmlns:ns398="http://docbook.org/ns/docbook">--full-time</ns398:entry><ns399:entry xmlns:ns399="http://docbook.org/ns/docbook">same as -l but timestamp in format of <ns400:emphasis xmlns:ns400="http://docbook.org/ns/docbook" role="strong">2003-08-04 09:58:57.000000000 +0200</ns400:emphasis></ns399:entry></ns397:row>
					<ns401:row xmlns:ns401="http://docbook.org/ns/docbook"><ns402:entry xmlns:ns402="http://docbook.org/ns/docbook">-h</ns402:entry><ns403:entry xmlns:ns403="http://docbook.org/ns/docbook">the filesize is converted to <ns404:quote xmlns:ns404="http://docbook.org/ns/docbook">human</ns404:quote> readable format using G, M, K to indicate Gigabyte, Megabyte and Kilobyte</ns403:entry></ns401:row>
					<ns405:row xmlns:ns405="http://docbook.org/ns/docbook"><ns406:entry xmlns:ns406="http://docbook.org/ns/docbook">-R</ns406:entry><ns407:entry xmlns:ns407="http://docbook.org/ns/docbook">list directories recursively</ns407:entry></ns405:row>
					<ns408:row xmlns:ns408="http://docbook.org/ns/docbook"><ns409:entry xmlns:ns409="http://docbook.org/ns/docbook">-t</ns409:entry><ns410:entry xmlns:ns410="http://docbook.org/ns/docbook">list files sorted by modified timestamp</ns410:entry></ns408:row>
					<ns411:row xmlns:ns411="http://docbook.org/ns/docbook"><ns412:entry xmlns:ns412="http://docbook.org/ns/docbook">-S</ns412:entry><ns413:entry xmlns:ns413="http://docbook.org/ns/docbook">list files sorted by file size</ns413:entry></ns411:row>
					<ns414:row xmlns:ns414="http://docbook.org/ns/docbook"><ns415:entry xmlns:ns415="http://docbook.org/ns/docbook">-r</ns415:entry><ns416:entry xmlns:ns416="http://docbook.org/ns/docbook">reverse sort order from descending to ascending</ns416:entry></ns414:row>
					<ns417:row xmlns:ns417="http://docbook.org/ns/docbook"><ns418:entry xmlns:ns418="http://docbook.org/ns/docbook">-1</ns418:entry><ns419:entry xmlns:ns419="http://docbook.org/ns/docbook">list one file per line</ns419:entry></ns417:row>
					<ns420:row xmlns:ns420="http://docbook.org/ns/docbook"><ns421:entry xmlns:ns421="http://docbook.org/ns/docbook">-Q</ns421:entry><ns422:entry xmlns:ns422="http://docbook.org/ns/docbook">list file names encapsulated by double quotes "</ns422:entry></ns420:row>
					<ns423:row xmlns:ns423="http://docbook.org/ns/docbook"><ns424:entry xmlns:ns424="http://docbook.org/ns/docbook">--color</ns424:entry><ns425:entry xmlns:ns425="http://docbook.org/ns/docbook">uses colour to categorize files, directories as well as permissions</ns425:entry></ns423:row>
				</ns379:tbody>
			</ns374:tgroup>
		</ns372:table>
		<ns426:para xmlns:ns426="http://docbook.org/ns/docbook">
			The <ns427:emphasis xmlns:ns427="http://docbook.org/ns/docbook" role="strong">context</ns427:emphasis> specifies the directory to list from. Multiple arguments can be used together. In the following example, a list of files are obtained with arguments to obtain additional detail (l), the size of the files in <ns428:quote xmlns:ns428="http://docbook.org/ns/docbook">human</ns428:quote> readable format (h), sorted by timestamp (t) in reversed order (r). 
		</ns426:para>
		<ns429:programlisting xmlns:ns429="http://docbook.org/ns/docbook">
alex@chameleon:/usr&gt; ls -lrth
total 97K
drwxr-xr-x    5 root     root          120 2003-03-14 00:22 i486-suse-linux
lrwxrwxrwx    1 root     root           10 2003-07-08 00:53 tmp -&gt; ../var/tmp
drwxr-xr-x    3 root     root           72 2003-07-08 00:53 src
drwxr-xr-x    8 root     root          192 2003-07-08 00:57 X11R6
lrwxrwxrwx    1 root     root            5 2003-07-08 01:00 X11 -&gt; X11R6
drwxr-xr-x    2 root     root          712 2003-07-08 01:23 games
drwxr-xr-x    4 root     root           96 2003-08-04 10:37 man
drwxr-xr-x   12 root     root          336 2003-08-11 17:35 local
drwxr-xr-x    2 root     root         9.1K 2003-08-12 10:47 sbin
drwxr-xr-x   93 root     root          33K 2003-08-12 10:47 lib
drwxr-xr-x   59 root     root         7.9K 2003-08-12 10:47 include
drwxr-xr-x  104 root     root         2.7K 2003-08-12 10:47 share
drwxr-xr-x    2 root     root          46K 2003-08-12 10:47 bin
alex@chameleon:/usr&gt; 
		</ns429:programlisting>	
	</ns363:section>	

	<ns430:section xmlns:ns430="http://docbook.org/ns/docbook">
		<ns431:title xmlns:ns431="http://docbook.org/ns/docbook">cp - Copy files and directories</ns431:title>
		<ns432:para xmlns:ns432="http://docbook.org/ns/docbook">
		copy files and directories from the source to the destination.
		</ns432:para>
		<ns433:screen xmlns:ns433="http://docbook.org/ns/docbook">
			<ns434:userinput xmlns:ns434="http://docbook.org/ns/docbook">
				<ns435:command xmlns:ns435="http://docbook.org/ns/docbook">cp</ns435:command> <ns436:replaceable xmlns:ns436="http://docbook.org/ns/docbook">flags</ns436:replaceable>  <ns437:replaceable xmlns:ns437="http://docbook.org/ns/docbook">source</ns437:replaceable> <ns438:replaceable xmlns:ns438="http://docbook.org/ns/docbook">destination</ns438:replaceable>
			</ns434:userinput>
		</ns433:screen>
		<ns439:table xmlns:ns439="http://docbook.org/ns/docbook" frame="all">
			<ns440:title xmlns:ns440="http://docbook.org/ns/docbook">cp Flags</ns440:title>
			<ns441:tgroup xmlns:ns441="http://docbook.org/ns/docbook" cols="2">
				<ns442:thead xmlns:ns442="http://docbook.org/ns/docbook">
					<ns443:row xmlns:ns443="http://docbook.org/ns/docbook"><ns444:entry xmlns:ns444="http://docbook.org/ns/docbook">Flag</ns444:entry><ns445:entry xmlns:ns445="http://docbook.org/ns/docbook">Description</ns445:entry></ns443:row>
				</ns442:thead>	
				<ns446:tbody xmlns:ns446="http://docbook.org/ns/docbook">
					<ns447:row xmlns:ns447="http://docbook.org/ns/docbook"><ns448:entry xmlns:ns448="http://docbook.org/ns/docbook">-f</ns448:entry><ns449:entry xmlns:ns449="http://docbook.org/ns/docbook">if the destination file cannot be opened, or is deleted</ns449:entry></ns447:row>
					<ns450:row xmlns:ns450="http://docbook.org/ns/docbook"><ns451:entry xmlns:ns451="http://docbook.org/ns/docbook">-i</ns451:entry><ns452:entry xmlns:ns452="http://docbook.org/ns/docbook">if the destination file exist cp will prompt before overwrite</ns452:entry></ns450:row>
					<ns453:row xmlns:ns453="http://docbook.org/ns/docbook"><ns454:entry xmlns:ns454="http://docbook.org/ns/docbook">-r</ns454:entry><ns455:entry xmlns:ns455="http://docbook.org/ns/docbook">copy directories recursively</ns455:entry></ns453:row>
					<ns456:row xmlns:ns456="http://docbook.org/ns/docbook"><ns457:entry xmlns:ns457="http://docbook.org/ns/docbook">-u</ns457:entry><ns458:entry xmlns:ns458="http://docbook.org/ns/docbook">only copy when the source file's timestamp is newer then the destination's timestamp</ns458:entry></ns456:row>
					<ns459:row xmlns:ns459="http://docbook.org/ns/docbook"><ns460:entry xmlns:ns460="http://docbook.org/ns/docbook">-v</ns460:entry><ns461:entry xmlns:ns461="http://docbook.org/ns/docbook">display files as they are being copied</ns461:entry></ns459:row>
				</ns446:tbody>
			</ns441:tgroup>
		</ns439:table>	
		<ns462:programlisting xmlns:ns462="http://docbook.org/ns/docbook">
alex@chameleon:~&gt; cp -v ical-2.2-28mdk.i586.rpm Mac/
`ical-2.2-28mdk.i586.rpm' -&gt; `Mac/ical-2.2-28mdk.i586.rpm'
alex@chameleon:~&gt; 
		</ns462:programlisting>
	</ns430:section>	

	<ns463:section xmlns:ns463="http://docbook.org/ns/docbook">
		<ns464:title xmlns:ns464="http://docbook.org/ns/docbook">cd - Changing directories</ns464:title>
		<ns465:para xmlns:ns465="http://docbook.org/ns/docbook">
		changes directory to the destination directory. 
		</ns465:para>
		<ns466:screen xmlns:ns466="http://docbook.org/ns/docbook">
			<ns467:userinput xmlns:ns467="http://docbook.org/ns/docbook">
				<ns468:command xmlns:ns468="http://docbook.org/ns/docbook">cd</ns468:command> <ns469:replaceable xmlns:ns469="http://docbook.org/ns/docbook">destination_directory</ns469:replaceable>
			</ns467:userinput>
		</ns466:screen>
		<ns470:programlisting xmlns:ns470="http://docbook.org/ns/docbook">
alex@chameleon:~&gt; cd /usr/local/bin/
alex@chameleon:/usr/local/bin&gt; 
		</ns470:programlisting>
	</ns463:section>	
	
	<ns471:section xmlns:ns471="http://docbook.org/ns/docbook">
		<ns472:title xmlns:ns472="http://docbook.org/ns/docbook">mv - Moving files and directories</ns472:title>
		<ns473:para xmlns:ns473="http://docbook.org/ns/docbook">
		moves the source files or directories to the destination files or into the destination directory. This command is also used to rename files and directories.
		</ns473:para>
		<ns474:screen xmlns:ns474="http://docbook.org/ns/docbook">
			<ns475:userinput xmlns:ns475="http://docbook.org/ns/docbook">
				<ns476:command xmlns:ns476="http://docbook.org/ns/docbook">mv</ns476:command> <ns477:replaceable xmlns:ns477="http://docbook.org/ns/docbook">flags</ns477:replaceable> <ns478:replaceable xmlns:ns478="http://docbook.org/ns/docbook">source</ns478:replaceable> <ns479:replaceable xmlns:ns479="http://docbook.org/ns/docbook">destination</ns479:replaceable>
			</ns475:userinput>
		</ns474:screen>
		<ns480:table xmlns:ns480="http://docbook.org/ns/docbook" frame="all">
			<ns481:title xmlns:ns481="http://docbook.org/ns/docbook">mv Flags</ns481:title>
			<ns482:tgroup xmlns:ns482="http://docbook.org/ns/docbook" cols="2">
				<ns483:thead xmlns:ns483="http://docbook.org/ns/docbook">
					<ns484:row xmlns:ns484="http://docbook.org/ns/docbook"><ns485:entry xmlns:ns485="http://docbook.org/ns/docbook">Flag</ns485:entry><ns486:entry xmlns:ns486="http://docbook.org/ns/docbook">Description</ns486:entry></ns484:row>
				</ns483:thead>	
				<ns487:tbody xmlns:ns487="http://docbook.org/ns/docbook">
					<ns488:row xmlns:ns488="http://docbook.org/ns/docbook"><ns489:entry xmlns:ns489="http://docbook.org/ns/docbook">-i</ns489:entry><ns490:entry xmlns:ns490="http://docbook.org/ns/docbook">will prompt before overwriting destination</ns490:entry></ns488:row>
					<ns491:row xmlns:ns491="http://docbook.org/ns/docbook"><ns492:entry xmlns:ns492="http://docbook.org/ns/docbook">-v</ns492:entry><ns493:entry xmlns:ns493="http://docbook.org/ns/docbook">displays files as they are being moved</ns493:entry></ns491:row>
					<ns494:row xmlns:ns494="http://docbook.org/ns/docbook"><ns495:entry xmlns:ns495="http://docbook.org/ns/docbook">-u</ns495:entry><ns496:entry xmlns:ns496="http://docbook.org/ns/docbook">only move source file if timestamp is newer than destination's timestamp</ns496:entry></ns494:row>
					<ns497:row xmlns:ns497="http://docbook.org/ns/docbook"><ns498:entry xmlns:ns498="http://docbook.org/ns/docbook">-f</ns498:entry><ns499:entry xmlns:ns499="http://docbook.org/ns/docbook">forces destination files to be overwritten without prompting</ns499:entry></ns497:row>
				</ns487:tbody>
			</ns482:tgroup>
		</ns480:table>	
		<ns500:programlisting xmlns:ns500="http://docbook.org/ns/docbook">
alex@chameleon:~&gt; mv -v ical-2.2-28mdk.i586.rpm Mac
`ical-2.2-28mdk.i586.rpm' -&gt; `Mac/ical-2.2-28mdk.i586.rpm'
alex@chameleon:~&gt; 
		</ns500:programlisting>
	</ns471:section>	
 		
	<ns501:section xmlns:ns501="http://docbook.org/ns/docbook" xml:id="resources.information.platforms.linux.commands.commands.rm">
		<ns502:title xmlns:ns502="http://docbook.org/ns/docbook">rm - Remove files and directories</ns502:title>
		<ns503:para xmlns:ns503="http://docbook.org/ns/docbook">
			Remove files and directories specified as target if you have permission. Attempts to delete a file without having write privileges results in a prompt, unless the <ns504:emphasis xmlns:ns504="http://docbook.org/ns/docbook" role="strong">-f</ns504:emphasis> flag is set.
		</ns503:para>
		<ns505:screen xmlns:ns505="http://docbook.org/ns/docbook">
			<ns506:userinput xmlns:ns506="http://docbook.org/ns/docbook">
				<ns507:command xmlns:ns507="http://docbook.org/ns/docbook">rm</ns507:command> <ns508:replaceable xmlns:ns508="http://docbook.org/ns/docbook">flags</ns508:replaceable> <ns509:replaceable xmlns:ns509="http://docbook.org/ns/docbook">target</ns509:replaceable>
			</ns506:userinput>
		</ns505:screen>
		<ns510:table xmlns:ns510="http://docbook.org/ns/docbook" frame="all">
			<ns511:title xmlns:ns511="http://docbook.org/ns/docbook">rm Flags</ns511:title>
			<ns512:tgroup xmlns:ns512="http://docbook.org/ns/docbook" cols="2">
				<ns513:thead xmlns:ns513="http://docbook.org/ns/docbook">
					<ns514:row xmlns:ns514="http://docbook.org/ns/docbook"><ns515:entry xmlns:ns515="http://docbook.org/ns/docbook">Flag</ns515:entry><ns516:entry xmlns:ns516="http://docbook.org/ns/docbook">Description</ns516:entry></ns514:row>
				</ns513:thead>	
				<ns517:tbody xmlns:ns517="http://docbook.org/ns/docbook">
					<ns518:row xmlns:ns518="http://docbook.org/ns/docbook"><ns519:entry xmlns:ns519="http://docbook.org/ns/docbook">-r</ns519:entry><ns520:entry xmlns:ns520="http://docbook.org/ns/docbook">remove files and directories recursively</ns520:entry></ns518:row>
					<ns521:row xmlns:ns521="http://docbook.org/ns/docbook"><ns522:entry xmlns:ns522="http://docbook.org/ns/docbook">-f</ns522:entry><ns523:entry xmlns:ns523="http://docbook.org/ns/docbook">do not prompt before deleting files</ns523:entry></ns521:row>
					<ns524:row xmlns:ns524="http://docbook.org/ns/docbook"><ns525:entry xmlns:ns525="http://docbook.org/ns/docbook">-i</ns525:entry><ns526:entry xmlns:ns526="http://docbook.org/ns/docbook">prompt before deleting each file</ns526:entry></ns524:row>
					<ns527:row xmlns:ns527="http://docbook.org/ns/docbook"><ns528:entry xmlns:ns528="http://docbook.org/ns/docbook">-v</ns528:entry><ns529:entry xmlns:ns529="http://docbook.org/ns/docbook">display files and directories as they are being deleted</ns529:entry></ns527:row>
				</ns517:tbody>
			</ns512:tgroup>
		</ns510:table>	
		<ns530:programlisting xmlns:ns530="http://docbook.org/ns/docbook">
alex@chameleon:~&gt; rm -v myVeryImportantDocument.txt
myVeryImportantDocument.txt
alex@chameleon:~&gt;
		</ns530:programlisting>
		<ns531:warning xmlns:ns531="http://docbook.org/ns/docbook">
		<ns532:para xmlns:ns532="http://docbook.org/ns/docbook">Using <ns533:emphasis xmlns:ns533="http://docbook.org/ns/docbook" role="strong">-f</ns533:emphasis> and <ns534:emphasis xmlns:ns534="http://docbook.org/ns/docbook" role="strong">-r</ns534:emphasis> as root user can be very dangerous, as all files at the target are deleted without prompting, as well as all recursive directories. Ensure that you are at the right place within the file system before issuing the rm command as root user.</ns532:para>
		</ns531:warning>
		<!--How do I undelete files in Linux-->
	</ns501:section>

	<ns535:section xmlns:ns535="http://docbook.org/ns/docbook">
		<ns536:title xmlns:ns536="http://docbook.org/ns/docbook">mkdir - Creating directories</ns536:title>
		<ns537:para xmlns:ns537="http://docbook.org/ns/docbook">
			Create a new directory at the specified target
		</ns537:para>
		<ns538:screen xmlns:ns538="http://docbook.org/ns/docbook">
			<ns539:userinput xmlns:ns539="http://docbook.org/ns/docbook">
				<ns540:command xmlns:ns540="http://docbook.org/ns/docbook">mk</ns540:command> <ns541:replaceable xmlns:ns541="http://docbook.org/ns/docbook">target_directory</ns541:replaceable>
			</ns539:userinput>
		</ns538:screen>
	</ns535:section>	

	<ns542:section xmlns:ns542="http://docbook.org/ns/docbook">
		<ns543:title xmlns:ns543="http://docbook.org/ns/docbook">rmdir - Removing directories</ns543:title>
		<ns544:para xmlns:ns544="http://docbook.org/ns/docbook">
			Remove the specified empty target directory. If the target directory is not empty, an error occurs. To delete directories and their contents, use <ns545:emphasis xmlns:ns545="http://docbook.org/ns/docbook" role="strong">rm -r</ns545:emphasis>.
		</ns544:para>
		<ns546:screen xmlns:ns546="http://docbook.org/ns/docbook">
			<ns547:userinput xmlns:ns547="http://docbook.org/ns/docbook">
				<ns548:command xmlns:ns548="http://docbook.org/ns/docbook">rmdir</ns548:command> <ns549:replaceable xmlns:ns549="http://docbook.org/ns/docbook">target_directory</ns549:replaceable>
			</ns547:userinput>
		</ns546:screen>
	</ns542:section>	

	<ns550:section xmlns:ns550="http://docbook.org/ns/docbook">
		<ns551:title xmlns:ns551="http://docbook.org/ns/docbook">cat - Viewing file contents</ns551:title>
		<ns552:para xmlns:ns552="http://docbook.org/ns/docbook">
			The source is read in and displayed on standard output. Specifying a source of <ns553:emphasis xmlns:ns553="http://docbook.org/ns/docbook" role="strong">-</ns553:emphasis> indicates to read from standard input and is used in piping, amongst others.
		</ns552:para>
		<ns554:screen xmlns:ns554="http://docbook.org/ns/docbook">
			<ns555:userinput xmlns:ns555="http://docbook.org/ns/docbook">
				<ns556:command xmlns:ns556="http://docbook.org/ns/docbook">cat</ns556:command> <ns557:replaceable xmlns:ns557="http://docbook.org/ns/docbook">flags</ns557:replaceable> <ns558:replaceable xmlns:ns558="http://docbook.org/ns/docbook">source</ns558:replaceable>
			</ns555:userinput>
		</ns554:screen>
		<ns559:table xmlns:ns559="http://docbook.org/ns/docbook" frame="all">
			<ns560:title xmlns:ns560="http://docbook.org/ns/docbook">cat Flags</ns560:title>
			<ns561:tgroup xmlns:ns561="http://docbook.org/ns/docbook" cols="2">
				<ns562:thead xmlns:ns562="http://docbook.org/ns/docbook">
					<ns563:row xmlns:ns563="http://docbook.org/ns/docbook"><ns564:entry xmlns:ns564="http://docbook.org/ns/docbook">Flag</ns564:entry><ns565:entry xmlns:ns565="http://docbook.org/ns/docbook">Description</ns565:entry></ns563:row>
				</ns562:thead>	
				<ns566:tbody xmlns:ns566="http://docbook.org/ns/docbook">
					<ns567:row xmlns:ns567="http://docbook.org/ns/docbook"><ns568:entry xmlns:ns568="http://docbook.org/ns/docbook">-n</ns568:entry><ns569:entry xmlns:ns569="http://docbook.org/ns/docbook">Lines are numbered, starting from 1</ns569:entry></ns567:row>
					<ns570:row xmlns:ns570="http://docbook.org/ns/docbook"><ns571:entry xmlns:ns571="http://docbook.org/ns/docbook">-b</ns571:entry><ns572:entry xmlns:ns572="http://docbook.org/ns/docbook">Empty lines are excluding from being numbered. This flag implies <ns573:emphasis xmlns:ns573="http://docbook.org/ns/docbook" role="strong">-n</ns573:emphasis></ns572:entry></ns570:row>
					<ns574:row xmlns:ns574="http://docbook.org/ns/docbook"><ns575:entry xmlns:ns575="http://docbook.org/ns/docbook">-s</ns575:entry><ns576:entry xmlns:ns576="http://docbook.org/ns/docbook">Multiple empty lines are squeezed / combined into one empty line</ns576:entry></ns574:row>
					<ns577:row xmlns:ns577="http://docbook.org/ns/docbook"><ns578:entry xmlns:ns578="http://docbook.org/ns/docbook">-u</ns578:entry><ns579:entry xmlns:ns579="http://docbook.org/ns/docbook">Output is unbuffered. Everything that is typed in is immediately displayed on standard out.</ns579:entry></ns577:row>
				</ns566:tbody>
			</ns561:tgroup>
		</ns559:table>	
		<ns580:programlisting xmlns:ns580="http://docbook.org/ns/docbook">
alex@chameleon:~&gt; cat -b -
d
     1  d


d
     2  d
alex@chameleon:~&gt; cat -n -

     1

     2
2
     3  2
3
     4  3
4
     5  4
alex@chameleon:~&gt; cat ~/.bashrc 
		</ns580:programlisting>
	</ns550:section>	

	<ns581:section xmlns:ns581="http://docbook.org/ns/docbook">
		<ns582:title xmlns:ns582="http://docbook.org/ns/docbook">more - Viewing file contents continued</ns582:title>
		<ns583:para xmlns:ns583="http://docbook.org/ns/docbook">
			To view a target file or stream, more is used. It allows for paging one screen at a time, by default, through the stream. Pressing <ns584:emphasis xmlns:ns584="http://docbook.org/ns/docbook" role="strong">enter</ns584:emphasis> moves one line further down, while pressing <ns585:emphasis xmlns:ns585="http://docbook.org/ns/docbook" role="strong">space bar</ns585:emphasis> moves an entire screen on. To quit more, either press <ns586:emphasis xmlns:ns586="http://docbook.org/ns/docbook" role="strong">CTRL+C</ns586:emphasis> or <ns587:emphasis xmlns:ns587="http://docbook.org/ns/docbook" role="strong">q</ns587:emphasis>. To search downwards within the target, press <ns588:emphasis xmlns:ns588="http://docbook.org/ns/docbook" role="strong">/</ns588:emphasis> followed by the expression. Only forward searching is supported. To access the online help feature, press the <ns589:emphasis xmlns:ns589="http://docbook.org/ns/docbook" role="strong">h</ns589:emphasis> key.
		</ns583:para>
		<ns590:screen xmlns:ns590="http://docbook.org/ns/docbook">
			<ns591:userinput xmlns:ns591="http://docbook.org/ns/docbook">
				<ns592:command xmlns:ns592="http://docbook.org/ns/docbook">more</ns592:command> <ns593:replaceable xmlns:ns593="http://docbook.org/ns/docbook">flags</ns593:replaceable> <ns594:replaceable xmlns:ns594="http://docbook.org/ns/docbook">target</ns594:replaceable>
			</ns591:userinput>
		</ns590:screen>
		<ns595:table xmlns:ns595="http://docbook.org/ns/docbook" frame="all">
			<ns596:title xmlns:ns596="http://docbook.org/ns/docbook">more Flags</ns596:title>
			<ns597:tgroup xmlns:ns597="http://docbook.org/ns/docbook" cols="2">
				<ns598:thead xmlns:ns598="http://docbook.org/ns/docbook">
					<ns599:row xmlns:ns599="http://docbook.org/ns/docbook"><ns600:entry xmlns:ns600="http://docbook.org/ns/docbook">Flag</ns600:entry><ns601:entry xmlns:ns601="http://docbook.org/ns/docbook">Description</ns601:entry></ns599:row>
				</ns598:thead>	
				<ns602:tbody xmlns:ns602="http://docbook.org/ns/docbook">
					<ns603:row xmlns:ns603="http://docbook.org/ns/docbook"><ns604:entry xmlns:ns604="http://docbook.org/ns/docbook">-n [number]</ns604:entry><ns605:entry xmlns:ns605="http://docbook.org/ns/docbook">Specifies the size in lines of the buffer</ns605:entry></ns603:row>
					<ns606:row xmlns:ns606="http://docbook.org/ns/docbook"><ns607:entry xmlns:ns607="http://docbook.org/ns/docbook">-s</ns607:entry><ns608:entry xmlns:ns608="http://docbook.org/ns/docbook">Squeezes / combines multiple sequential blank lines into a single blank line</ns608:entry></ns606:row>
				</ns602:tbody>
			</ns597:tgroup>
		</ns595:table>	
		<ns609:note xmlns:ns609="http://docbook.org/ns/docbook">
		<ns610:para xmlns:ns610="http://docbook.org/ns/docbook">more may also be used to paginate output by piping the output to more. ie : ls -l |more</ns610:para>
		</ns609:note>
	</ns581:section>	

	<ns611:section xmlns:ns611="http://docbook.org/ns/docbook">
		<ns612:title xmlns:ns612="http://docbook.org/ns/docbook">less - Viewing file contents (less is more)</ns612:title>
		<ns613:para xmlns:ns613="http://docbook.org/ns/docbook">
		The <ns614:emphasis xmlns:ns614="http://docbook.org/ns/docbook" role="strong">less</ns614:emphasis> utility provides a major benefit over <ns615:emphasis xmlns:ns615="http://docbook.org/ns/docbook" role="strong">more</ns615:emphasis> in that it allows backwards scrolling over the buffer. Use the <ns616:emphasis xmlns:ns616="http://docbook.org/ns/docbook" role="strong">up</ns616:emphasis> and <ns617:emphasis xmlns:ns617="http://docbook.org/ns/docbook" role="strong">down</ns617:emphasis> keys to scroll up or down through the target. To search for the target in any direction press <ns618:emphasis xmlns:ns618="http://docbook.org/ns/docbook" role="strong">/</ns618:emphasis> followed by the expression. Online help is available by pressing the <ns619:emphasis xmlns:ns619="http://docbook.org/ns/docbook" role="strong">h</ns619:emphasis> key.
		</ns613:para>
		<ns620:screen xmlns:ns620="http://docbook.org/ns/docbook">
			<ns621:userinput xmlns:ns621="http://docbook.org/ns/docbook">
				<ns622:command xmlns:ns622="http://docbook.org/ns/docbook">less</ns622:command> <ns623:replaceable xmlns:ns623="http://docbook.org/ns/docbook">flags</ns623:replaceable> <ns624:replaceable xmlns:ns624="http://docbook.org/ns/docbook">target</ns624:replaceable>
			</ns621:userinput>
		</ns620:screen>
		<ns625:note xmlns:ns625="http://docbook.org/ns/docbook">
		<ns626:para xmlns:ns626="http://docbook.org/ns/docbook">more may also be used to paginate output by piping the output to more. ie : ls -l |less</ns626:para>
		</ns625:note>
	</ns611:section>
	
	<ns627:section xmlns:ns627="http://docbook.org/ns/docbook">
		<ns628:title xmlns:ns628="http://docbook.org/ns/docbook">head - Viewing the first few lines of a file</ns628:title>
		<ns629:para xmlns:ns629="http://docbook.org/ns/docbook">
			Displays the first n number of lines of a specified target. If no number (n) is specified, the default of 10 lines is displayed.
		</ns629:para>
		<ns630:screen xmlns:ns630="http://docbook.org/ns/docbook">
			<ns631:userinput xmlns:ns631="http://docbook.org/ns/docbook">
				<ns632:command xmlns:ns632="http://docbook.org/ns/docbook">head</ns632:command> <ns633:replaceable xmlns:ns633="http://docbook.org/ns/docbook">-n [number]</ns633:replaceable> <ns634:replaceable xmlns:ns634="http://docbook.org/ns/docbook">target</ns634:replaceable>
			</ns631:userinput>
		</ns630:screen>
		<ns635:programlisting xmlns:ns635="http://docbook.org/ns/docbook">
alex@chameleon:~&gt; head ~/.bash_history 
ls
exit
exit
export CVSROOT=:ext:alex@bluescontrol.co.za:/home/cvsadmin/CVSROOT
export CVS_RSH=ssh
cvs login
export CVSROOT=:ext:alex@bluescontrol.co.za:/home/cvsadmin/CVSROOT
cvs login
cvs login
set
alex@chameleon:~&gt;
		</ns635:programlisting>
	</ns627:section>	

	<ns636:section xmlns:ns636="http://docbook.org/ns/docbook">
		<ns637:title xmlns:ns637="http://docbook.org/ns/docbook">tail - Viewing the tail end of a file</ns637:title>
		<ns638:para xmlns:ns638="http://docbook.org/ns/docbook">
			Opposite of <ns639:emphasis xmlns:ns639="http://docbook.org/ns/docbook" role="strong">head</ns639:emphasis>, tail displays the tail end of a file. In addition to head, tail also offer a set of useful features. <ns640:emphasis xmlns:ns640="http://docbook.org/ns/docbook" role="strong">tail</ns640:emphasis> allows for the continued monitoring of the tail end of a target, thus being especially useful for the monitoring of log files.
		</ns638:para>
		<ns641:screen xmlns:ns641="http://docbook.org/ns/docbook">
			<ns642:userinput xmlns:ns642="http://docbook.org/ns/docbook">
				<ns643:command xmlns:ns643="http://docbook.org/ns/docbook">tail</ns643:command> <ns644:replaceable xmlns:ns644="http://docbook.org/ns/docbook">flags</ns644:replaceable> <ns645:replaceable xmlns:ns645="http://docbook.org/ns/docbook">target</ns645:replaceable>
			</ns642:userinput>
		</ns641:screen>
		<ns646:table xmlns:ns646="http://docbook.org/ns/docbook" frame="all">
			<ns647:title xmlns:ns647="http://docbook.org/ns/docbook">tail Flags</ns647:title>
			<ns648:tgroup xmlns:ns648="http://docbook.org/ns/docbook" cols="2">
				<ns649:thead xmlns:ns649="http://docbook.org/ns/docbook">
					<ns650:row xmlns:ns650="http://docbook.org/ns/docbook"><ns651:entry xmlns:ns651="http://docbook.org/ns/docbook">Flag</ns651:entry><ns652:entry xmlns:ns652="http://docbook.org/ns/docbook">Description</ns652:entry></ns650:row>
				</ns649:thead>	
				<ns653:tbody xmlns:ns653="http://docbook.org/ns/docbook">
					<ns654:row xmlns:ns654="http://docbook.org/ns/docbook"><ns655:entry xmlns:ns655="http://docbook.org/ns/docbook">-n [number]</ns655:entry><ns656:entry xmlns:ns656="http://docbook.org/ns/docbook">Displays the last <ns657:emphasis xmlns:ns657="http://docbook.org/ns/docbook" role="strong">number</ns657:emphasis> of lines of the target</ns656:entry></ns654:row>
					<ns658:row xmlns:ns658="http://docbook.org/ns/docbook"><ns659:entry xmlns:ns659="http://docbook.org/ns/docbook">-f</ns659:entry><ns660:entry xmlns:ns660="http://docbook.org/ns/docbook">tail will continue to monitor the last lines of the target until <ns661:emphasis xmlns:ns661="http://docbook.org/ns/docbook" role="strong">CTRL+C</ns661:emphasis> or <ns662:emphasis xmlns:ns662="http://docbook.org/ns/docbook" role="strong">q</ns662:emphasis> are pressed.</ns660:entry></ns658:row>
					<ns663:row xmlns:ns663="http://docbook.org/ns/docbook"><ns664:entry xmlns:ns664="http://docbook.org/ns/docbook">-F</ns664:entry><ns665:entry xmlns:ns665="http://docbook.org/ns/docbook">same as <ns666:emphasis xmlns:ns666="http://docbook.org/ns/docbook" role="strong">-f</ns666:emphasis> with the additional feature that it caters for rolling files. If the current target (eg mylog) had been renamed (to eg mylog.0) this option is used to continue monitoring the newly created my target (eg mylog).</ns665:entry></ns663:row>
				</ns653:tbody>
			</ns648:tgroup>
		</ns646:table>	
		<ns667:programlisting xmlns:ns667="http://docbook.org/ns/docbook">
alex@chameleon:~&gt; tail ~/.tcshrc 
set history=10000 savehist=10000
setenv CVSROOT /Users/alex/cvsRepository/
setenv JAVA_HOME /usr/
alex@chameleon:~&gt; 
		</ns667:programlisting>
	</ns636:section>
	
	<ns668:section xmlns:ns668="http://docbook.org/ns/docbook">
		<ns669:title xmlns:ns669="http://docbook.org/ns/docbook">Finding files with find</ns669:title>
		<ns670:para xmlns:ns670="http://docbook.org/ns/docbook">
			To search a context for a file matching a specified pattern, use the find command. Depending on the specified context and disk size / usage of this command may run for awhile. 
		</ns670:para>
		<ns671:screen xmlns:ns671="http://docbook.org/ns/docbook">
			<ns672:userinput xmlns:ns672="http://docbook.org/ns/docbook">
				<ns673:command xmlns:ns673="http://docbook.org/ns/docbook">find</ns673:command> <ns674:replaceable xmlns:ns674="http://docbook.org/ns/docbook">context</ns674:replaceable> <ns675:replaceable xmlns:ns675="http://docbook.org/ns/docbook">flag</ns675:replaceable> <ns676:replaceable xmlns:ns676="http://docbook.org/ns/docbook">search_pattern</ns676:replaceable> 
			</ns672:userinput>
		</ns671:screen>
		<ns677:table xmlns:ns677="http://docbook.org/ns/docbook" frame="all">
			<ns678:title xmlns:ns678="http://docbook.org/ns/docbook">find Flags</ns678:title>
			<ns679:tgroup xmlns:ns679="http://docbook.org/ns/docbook" cols="2">
				<ns680:thead xmlns:ns680="http://docbook.org/ns/docbook">
					<ns681:row xmlns:ns681="http://docbook.org/ns/docbook"><ns682:entry xmlns:ns682="http://docbook.org/ns/docbook">Flag</ns682:entry><ns683:entry xmlns:ns683="http://docbook.org/ns/docbook">Description</ns683:entry></ns681:row>
				</ns680:thead>	
				<ns684:tbody xmlns:ns684="http://docbook.org/ns/docbook">
					<ns685:row xmlns:ns685="http://docbook.org/ns/docbook"><ns686:entry xmlns:ns686="http://docbook.org/ns/docbook"/><ns687:entry xmlns:ns687="http://docbook.org/ns/docbook"/></ns685:row>
				</ns684:tbody>
			</ns679:tgroup>
		</ns677:table>	
		<ns688:table xmlns:ns688="http://docbook.org/ns/docbook" frame="all">
			<ns689:title xmlns:ns689="http://docbook.org/ns/docbook">find Search patterns</ns689:title>
			<ns690:tgroup xmlns:ns690="http://docbook.org/ns/docbook" cols="2">
				<ns691:thead xmlns:ns691="http://docbook.org/ns/docbook">
					<ns692:row xmlns:ns692="http://docbook.org/ns/docbook"><ns693:entry xmlns:ns693="http://docbook.org/ns/docbook">Search Pattern</ns693:entry><ns694:entry xmlns:ns694="http://docbook.org/ns/docbook">Action</ns694:entry></ns692:row>
				</ns691:thead>	
				<ns695:tbody xmlns:ns695="http://docbook.org/ns/docbook">
					<ns696:row xmlns:ns696="http://docbook.org/ns/docbook"><ns697:entry xmlns:ns697="http://docbook.org/ns/docbook">-name [filename]</ns697:entry><ns698:entry xmlns:ns698="http://docbook.org/ns/docbook">Searches file names</ns698:entry></ns696:row>
					<ns699:row xmlns:ns699="http://docbook.org/ns/docbook"><ns700:entry xmlns:ns700="http://docbook.org/ns/docbook">-iname [filename]</ns700:entry><ns701:entry xmlns:ns701="http://docbook.org/ns/docbook">Searches file names ignoring case</ns701:entry></ns699:row>
					<ns702:row xmlns:ns702="http://docbook.org/ns/docbook"><ns703:entry xmlns:ns703="http://docbook.org/ns/docbook">-user [user]</ns703:entry><ns704:entry xmlns:ns704="http://docbook.org/ns/docbook">Searches files owned by user</ns704:entry></ns702:row>
					<ns705:row xmlns:ns705="http://docbook.org/ns/docbook"><ns706:entry xmlns:ns706="http://docbook.org/ns/docbook">-group [group]</ns706:entry><ns707:entry xmlns:ns707="http://docbook.org/ns/docbook">Searches files belonging to group</ns707:entry></ns705:row>
				</ns695:tbody>
			</ns690:tgroup>
