  <ns892:section xmlns:ns892="http://docbook.org/ns/docbook" xml:id="resources.information.solms.training.courses.Security.targetAudience">
    <ns893:title xmlns:ns893="http://docbook.org/ns/docbook">For whom is the course meant?</ns893:title>
    <ns894:para xmlns:ns894="http://docbook.org/ns/docbook">
